<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">Quick Search Box</string>

    <!-- Search widget -->
    <string name="search_widget">Quick Search Box</string>
    <string name="search_widget_hint">Quick Search Box</string>

    <!-- Source selector -->
    <string name="global_search_label">Everything</string>

    <!-- Search settings stuff -->

    <!-- Search settings menu item -->
    <string name="menu_settings">Search settings</string>

    <!-- Search settings title -->
    <string name="search_settings">Search settings</string>

    <!-- Title for 'web search' category of search settings -->
    <string name="web_search_category_title">Web search</string>

    <!-- Title and summary for 'search engine' setting -->
    <string name="web_search_source">Web search engine</string>
    <string name="web_search_source_summary"><xliff:g id="source" example="Google">%1$s</xliff:g></string>

    <!-- Title and summaries for 'search engine settings' link (if available for the chosen
         web search source) -->
    <string name="search_engine_settings">Search engine settings</string>
    <string name="search_engine_settings_summary_enabled">Settings for <xliff:g id="provider" example="Google">%1$s</xliff:g></string>
    <string name="search_engine_settings_summary_disabled">No settings for <xliff:g id="provider" example="Google">%1$s</xliff:g></string>

    <!-- Title for 'system search' category of search settings -->
    <string name="system_search_category_title">Quick Search Box</string>

    <!-- Title and summary for 'search sources' link for system search -->
    <string name="search_sources">Searchable items</string>
    <string name="search_sources_summary">Choose what to search on the phone</string>

    <!-- Title and summary used for the option to clear search shortcuts from the system
         search history -->
    <string name="clear_shortcuts">Clear search shortcuts</string>
    <string name="clear_shortcuts_summary">Clear shortcuts to recently chosen search results</string>

    <!-- Strings used in the dialog shown after choosing the 'clear search shortcuts' setting. -->
    <string name="clear_shortcuts_prompt">"This will clear all shortcuts to recently chosen search results."\n\n"(To clear your Browser history, use the Browser's settings.)"</string>
    <string name="agree">OK</string>
    <string name="disagree">Cancel</string>

</resources>
